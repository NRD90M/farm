<view><view class='header'><view class='header_wrapper'><view class='header_info_wrapper'><image class='userAvatarUrl' src='{{user.avatarurl}}'></image><text class='userNickName'>{{user.nickname}}</text></view></view></view><movable-area style='height: {{screenHeight - 180 * Proportion}}px; width: 100%; background:transparent; margin-top: 180rpx;'><movable-view direction='all' scale='true' scaleArea='true' scaleMax='3' scaleMin='1' style='height: 120%; width: 120%;'><view class='mian' style='height:100%'><image class='main_bg' src='../../images/farm/main_bg.jpg'></image><view bindtap='shopMall' class='{{(isFullScreen?"house":"houses")}}'></view><view class='lands_wrapper' style='top:{{(isFullScreen?530:"")}}rpx'><view class='lands_array'><image class='lands_img' src='../../images/farm/lands.png'></image><view class='lands'><block wx:for='{{lands}}' wx:for-item='item' wx:for-index='idx' wx:key='id'><view class='land_item'><view class='land_detail'><image class='landImg' src='../../images/farm/{{(item.is_land_buy === 0?"land":"selectLand")}}.png'></image><block wx:if='{{item.crops.length > 0}}'><block wx:if='{{item.process == 2}}'><image class='plantImg' src='../../images/farm/harvest.png'></image></block><block wx:elif='{{ (item.process == 1 || item.process == 3) }}'><image class='plantImg' src='../../images/farm/plant.png'></image></block><block wx:if='{{item.process == 1}}'><view class='growthIng'><image class='star' src='../../images/farm/star.png'></image><text>成长中</text></view></block><block wx:if='{{ (item.process == 2 && !(item.steal)) }}'><view catchtap='mature' class='mature' data-id='{{item.id}}'><view>已成熟</view><image src='../../images/farm/handle.png'></image></view></block><block wx:if='{{ (item.process == 3 && item.animation) }}'><view class='handImgs'><view>正在收获中</view><image src='../../images/farm/hourglass.png'></image></view></block></block></view></view></block></view></view></view><pond isFullScreen='{{isFullScreen}}' isrun='{{isrun}}'></pond><chicken2 isFullScreen='{{isFullScreen}}' isrun='{{isrun}}'></chicken2><chicken1 isFullScreen='{{isFullScreen}}' isrun='{{isrun}}'></chicken1><duck isFullScreen='{{isFullScreen}}' isrun='{{isrun}}'></duck></view></movable-view></movable-area><view class='left_wrapper'><image bindtap='checkFriend' src='../../images/farm/friend.png'></image><image src='../../images/farm/Invitation.png'><button class='shareBtn' openType='share'></button></image></view><view class='friend_wrapper {{(showFriend?"show":"hide")}}'><view class='headLine'></view><image catchtap='checkFriend' class='closeImg' src='../../images/farm/close.png'></image><view class='friend_list'><block wx:for='{{friendList}}' wx:for-item='item' wx:for-index='index' wx:key='{{index}}'><view class='friend_item'><image class='friend_item_avator' src='{{item.avatarurl}}'></image><view class='friend_name'>{{item.nickname}}</view><form bindsubmit='visited' data-frienduid='{{item.uid}}' reportSubmit='true'><button class='visited' formType='submit'>拜访</button></form></view></block></view></view><view bindtap='goBack' class='gohome' style='bottom:{{(isFullScreen?20:0)}}rpx'><view class='userAvatarUrls'><open-data type='userAvatarUrl'></open-data></view><view><image class='gohomeImg' src='../../images/farm/gohome.png'></image></view></view></view>