.animal_wrapper{ position: fixed; right: 350rpx; top: 280rpx; }
.cicken3{ width: 50rpx; height: 46rpx; -webkit-animation: run 12s linear infinite; animation: run 12s linear infinite; }
.top{ top:370rpx; }
@-webkit-keyframes run{ 49%{ -webkit-transform: translateX(30px); transform: translateX(30px) }
50%{ -webkit-transform: translateX(30px) rotateY(-180deg); transform: translateX(30px) rotateY(-180deg) }
99%{ -webkit-transform: translateX(0px) rotateY(-180deg); transform: translateX(0px) rotateY(-180deg) }
100%{ -webkit-transform: rotateY(0deg); transform: rotateY(0deg) }
}@keyframes run{ 49%{ -webkit-transform: translateX(30px); transform: translateX(30px) }
50%{ -webkit-transform: translateX(30px) rotateY(-180deg); transform: translateX(30px) rotateY(-180deg) }
99%{ -webkit-transform: translateX(0px) rotateY(-180deg); transform: translateX(0px) rotateY(-180deg) }
100%{ -webkit-transform: rotateY(0deg); transform: rotateY(0deg) }
}