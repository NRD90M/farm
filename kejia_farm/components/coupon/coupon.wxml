<view class='coupon_container'><view class='flex'><text class='iconfont icon-coupon f48' style='color:{{setData.background_color}}'></text><view class='coupon_title' style='color:{{setData.background_color}}'>专享优惠券</view></view><view bindtap='examineMoreCoupon' class='flex'><view style='color:gray;'>更多</view><text class='iconfont icon-right right-icon'></text></view></view><view class='coupons'><scroll-view class='coupons_container' scrollX='true'><block wx:for='{{couponData}}' wx:for-item='item' wx:for-index='index' wx:key='id'><view bindtap='getCoupon' class='coupon_item' data-cid='{{item.id}}' data-type='{{item.type}}'><block wx:if='{{item.is_get == 1}}'><image class='voucher' src='{{noIcon}}'></image></block><block wx:else><image class='voucher' src='{{icon}}'></image></block><view class='voucher_container'><view class='voucher_desc'><view class='voucher_price'><text class='f30'>￥</text>{{item.coupon_price}}</view><view class='condition'>满{{item.low_cash_price}}元可用</view></view><block wx:if='{{item.is_get == 1}}'><view class='receive is-get'><text>已</text><text>领</text><text>取</text></view></block><block wx:else><view class='receive'><text>立</text><text>即</text><text>领</text><text>取</text></view></block></view></view></block></scroll-view></view>