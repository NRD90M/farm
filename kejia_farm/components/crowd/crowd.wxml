<view><view class='mian'><block wx:for='{{lists}}' wx:for-item='item' wx:for-index='index' wx:key='id'><view bindtap='intoFundingDetail' class='list_wrapper' data-projectid='{{item.id}}'><view class='pic_wrapper'><image class='item_pic' src='{{item.cover}}'></image><block wx:if='{{item.hot}}'><view class='iconfont icon-hot sign' style='color: #19BB65'></view></block></view><view class='desc_wrapper'><view class='item_name_wrapper'><view class='item_name'>{{item.project_name}}</view><block wx:if='{{item.project_status == 0}}'><view class='item_state item_states'>未开始</view></block><block wx:if='{{item.project_status == 1}}'><view class='item_state item_states'>已结束</view></block><block wx:if='{{item.project_status == 2}}'><view class='item_state'>进行中</view></block><block wx:if='{{item.project_status == 3}}'><view class='item_state item_states'>众筹成功</view></block></view><view class='crowd_info_wrapper'><view><view class='raise_wrapper'>周期:{{item.cycle}}</view><view class='raise_wrapper'>目标金额:￥{{item.target_money}}</view><view class='raise_wrapper'>认筹档位:￥{{item.min_price}}起</view></view><view class='crowd_item_rate'><view>{{item.return_percent}}%</view><view>年化收益率</view></view></view></view></view></block></view></view>