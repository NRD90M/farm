<view>
    <view class='head_container' style='background:{{farmSetData.background_color}}'></view>
    <view class='addInfo_container'>
        <view class='addTitle'>填写信息</view>
        <view class='mt30'>
            <form bindsubmit='submitInfo'>
                <view class='input_container'>
                    <input name='card_num' placeholder='投资人' type='text' value='{{card_num}}'></input>
                </view>
                <view class='input_container'>
                    <input name='card_pwd'  placeholder='手机号' type='text' value='{{card_pwd}}'></input>
                </view>
                <button class='confrim' formType='submit' style='background:{{farmSetData.background_color}}'>
                    提交申请
                </button>
            </form>
        </view>
    </view>
</view>